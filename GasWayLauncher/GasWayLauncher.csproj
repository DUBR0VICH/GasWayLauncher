  </ItemGroup>
  <ItemGroup>
    <Resource Include="View\Res\Icons\close.png" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="View\Res\Icons\razvert.png" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="View\Res\Icons\zavert.png" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="View\Res\Background\korob.jpg" />
