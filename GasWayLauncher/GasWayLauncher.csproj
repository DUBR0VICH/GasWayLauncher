  <ItemGroup>
    <Resource Include="View\Res\Icons\strelka.png" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="View\Res\Icons\planet.png" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="View\Res\Icons\settings.png" />
    <Resource Include="View\Res\Icons\settings2.png" />
